{
  "@context": {
    "@vocab": "https://becknprotocol.io/schema/",
    "beckn": "https://becknprotocol.io/schema/",
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#"
  },
  "@id": "https://becknprotocol.io/schema/AddOn",
  "@type": "owl:Class",
  "rdfs:label": "Add On",
  "rdfs:comment": "Defines additional products or services that can be added to a retail item",
  "rdfs:subClassOf": "owl:Thing",
  "rdfs:isDefinedBy": "https://becknprotocol.io/schema/",
  "owl:equivalentClass": {
    "@type": "owl:Class",
    "owl:intersectionOf": [
      "owl:Thing",
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:addOnId",
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:addOnName",
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:addOnType",
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:addOnPrice",
        "owl:allValuesFrom": "beckn:Price"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:addOnDescription",
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:addOnRequired",
        "owl:allValuesFrom": "schema:Boolean"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:addOnAvailable",
        "owl:allValuesFrom": "schema:Boolean"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:addOnCategory",
        "owl:allValuesFrom": "schema:Text"
      }
    ]
  }
}
