{
  "@context": {
    "@vocab": "https://becknprotocol.io/schema/",
    "beckn": "https://becknprotocol.io/schema/",
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#"
  },
  "@id": "https://becknprotocol.io/schema/PackagingInfo",
  "@type": "owl:Class",
  "rdfs:label": "Packaging Information",
  "rdfs:comment": "Defines packaging details including weight, dimensions, and special handling requirements",
  "rdfs:subClassOf": "owl:Thing",
  "rdfs:isDefinedBy": "https://becknprotocol.io/schema/",
  "owl:equivalentClass": {
    "@type": "owl:Class",
    "owl:intersectionOf": [
      "owl:Thing",
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:packageWeight",
          "rdfs:label": "Package Weight",
          "rdfs:comment": "The weight of the packaged item"
        },
        "owl:allValuesFrom": "schema:Number"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:packageDimensions",
          "rdfs:label": "Package Dimensions",
          "rdfs:comment": "Length, width, height, and other dimensional information"
        },
        "owl:allValuesFrom": "beckn:Dimensions"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:packagingType",
          "rdfs:label": "Packaging Type",
          "rdfs:comment": "The type of packaging used (e.g., box, envelope, bag, crate)"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:fragile",
          "rdfs:label": "Fragile",
          "rdfs:comment": "Whether the package contains fragile items requiring careful handling"
        },
        "owl:allValuesFrom": "schema:Boolean"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:specialHandlingRequired",
          "rdfs:label": "Special Handling Required",
          "rdfs:comment": "Whether the package requires special handling procedures"
        },
        "owl:allValuesFrom": "schema:Boolean"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:temperatureControlled",
          "rdfs:label": "Temperature Controlled",
          "rdfs:comment": "Whether the package requires temperature-controlled shipping"
        },
        "owl:allValuesFrom": "schema:Boolean"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:maxStackHeight",
          "rdfs:label": "Maximum Stack Height",
          "rdfs:comment": "Maximum number of packages that can be stacked safely"
        },
        "owl:allValuesFrom": "schema:Integer"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:packagingMaterial",
          "rdfs:label": "Packaging Material",
          "rdfs:comment": "The material used for packaging (e.g., cardboard, plastic, wood)"
        },
        "owl:allValuesFrom": "schema:Text"
      }
    ]
  }
}
