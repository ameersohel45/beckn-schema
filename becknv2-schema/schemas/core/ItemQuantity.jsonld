{
  "@context": {
    "@vocab": "https://becknprotocol.io/schema/",
    "beckn": "https://becknprotocol.io/schema/",
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#"
  },
  "@id": "https://becknprotocol.io/schema/ItemQuantity",
  "@type": "owl:Class",
  "rdfs:label": "Item Quantity",
  "rdfs:comment": "Defines quantity information for retail items including count, measure, and units",
  "rdfs:subClassOf": "owl:Thing",
  "rdfs:isDefinedBy": "https://becknprotocol.io/schema/",
  "owl:equivalentClass": {
    "@type": "owl:Class",
    "owl:intersectionOf": [
      "owl:Thing",
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:count",
          "rdfs:label": "Count",
          "rdfs:comment": "The number of items in this quantity"
        },
        "owl:allValuesFrom": "schema:Integer"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:measure",
          "rdfs:label": "Measure",
          "rdfs:comment": "The numerical measurement value of the quantity"
        },
        "owl:allValuesFrom": "schema:Number"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:unit",
          "rdfs:label": "Unit",
          "rdfs:comment": "The unit of measurement (e.g., kg, ml, pieces)"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:available",
          "rdfs:label": "Available",
          "rdfs:comment": "Whether this quantity is currently available for purchase"
        },
        "owl:allValuesFrom": "schema:Boolean"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:minimum",
          "rdfs:label": "Minimum",
          "rdfs:comment": "The minimum quantity that can be ordered"
        },
        "owl:allValuesFrom": "schema:Integer"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:maximum",
          "rdfs:label": "Maximum",
          "rdfs:comment": "The maximum quantity that can be ordered"
        },
        "owl:allValuesFrom": "schema:Integer"
      }
    ]
  }
}
