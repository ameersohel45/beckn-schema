{
  "@context": {
    "@vocab": "https://becknprotocol.io/schema/",
    "beckn": "https://becknprotocol.io/schema/",
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#"
  },
  "@id": "https://becknprotocol.io/schema/Descriptor",
  "@type": "owl:Class",
  "rdfs:label": "Descriptor",
  "rdfs:comment": "Physical description of something",
  "rdfs:subClassOf": "schema:Thing",
  "rdfs:isDefinedBy": "https://becknprotocol.io/schema/",
  "owl:equivalentClass": {
    "@type": "owl:Class",
    "owl:intersectionOf": [
      "schema:Thing",
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "schema:name",
          "rdfs:label": "Descriptor Name",
          "rdfs:comment": "The name or title of the described entity"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:code",
          "rdfs:label": "Descriptor Code",
          "rdfs:comment": "A code or identifier for the descriptor"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:shortDesc",
          "rdfs:label": "Short Description",
          "rdfs:comment": "A brief, concise description of the entity"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:longDesc",
          "rdfs:label": "Long Description",
          "rdfs:comment": "A detailed, comprehensive description of the entity"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:additionalDesc",
          "rdfs:label": "Additional Description",
          "rdfs:comment": "Supplementary description with URL and content type information"
        },
        "owl:allValuesFrom": {
          "@type": "owl:Class",
          "owl:intersectionOf": [
            "schema:Thing",
            {
              "@type": "owl:Restriction",
              "owl:onProperty": {
                "@id": "schema:url",
                "rdfs:label": "Description URL",
                "rdfs:comment": "URL to additional description content"
              },
              "owl:allValuesFrom": "schema:URL"
            },
            {
              "@type": "owl:Restriction",
              "owl:onProperty": {
                "@id": "beckn:contentType",
                "rdfs:label": "Content Type",
                "rdfs:comment": "The type of content (text/plain, text/html, application/json)"
              },
              "owl:allValuesFrom": {
                "@type": "owl:Class",
                "owl:oneOf": [
                  {"@id": "beckn:textPlain"},
                  {"@id": "beckn:textHtml"},
                  {"@id": "beckn:applicationJson"}
                ]
              }
            }
          ]
        }
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:media",
          "rdfs:label": "Media Files",
          "rdfs:comment": "Media files associated with the descriptor"
        },
        "owl:allValuesFrom": "beckn:MediaFile"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:images",
          "rdfs:label": "Images",
          "rdfs:comment": "Images associated with the descriptor"
        },
        "owl:allValuesFrom": "schema:ImageObject"
      }
    ]
  }
}
