  "beckn:categoryAttributes": {
    "@type": "owl:Class",
    "rdfs:label": "Category Attributes",
    "rdfs:comment": "Additional attributes that can be associated with categories",
    
    "beckn:perishable": {
      "@type": "schema:Boolean",
      "rdfs:comment": "Whether items in this category are perishable"
    },
    "beckn:storageType": {
      "@type": "schema:Text",
      "rdfs:comment": "Recommended storage type (refrigerated, frozen, shelf_stable, etc.)"
    },
    "beckn:level": {
      "@type": "schema:Number",
      "rdfs:comment": "Hierarchy level of the category"
    },
    "beckn:parentCategory": {
      "@type": "schema:Text",
      "rdfs:comment": "Parent category code"
    },
    "beckn:requiresSpecialHandling": {
      "@type": "schema:Boolean",
      "rdfs:comment": "Whether items require special handling"
    },
    "beckn:seasonal": {
      "@type": "schema:Boolean",
      "rdfs:comment": "Whether items are seasonal"
    }
  },
  
  "beckn:categoryMapping": {
    "@type": "owl:Class",
    "rdfs:label": "Category Mapping",
    "rdfs:comment": "Intelligent mapping between item types and categories",
    
    "beckn:electronicItems": {
      "beckn:primaryCategory": "ELECTRONICS",
      "beckn:subCategory": "SMARTPHONES",
      "beckn:itemType": "beckn:ElectronicItem",
      "beckn:defaultAttributes": {
        "beckn:perishable": false,
        "beckn:storageType": "room_temperature",
        "beckn:requiresSpecialHandling": true,
        "beckn:seasonal": false
      }
    },
    
    "beckn:groceryItems": {
      "beckn:primaryCategory": "GROCERY",
      "beckn:subCategory": "FRESH_FRUITS",
      "beckn:itemType": "beckn:GroceryItem",
      "beckn:defaultAttributes": {
        "beckn:perishable": true,
        "beckn:storageType": "refrigerated",
        "beckn:requiresSpecialHandling": true,
        "beckn:seasonal": true
      }
    }
