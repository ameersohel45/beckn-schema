{
  "@context": {
    "@vocab": "https://becknprotocol.io/schema/",
    "beckn": "https://becknprotocol.io/schema/",
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#"
  },
  "@id": "https://becknprotocol.io/schema/Catalog",
  "@type": "owl:Class",
  "rdfs:label": "Catalog",
  "rdfs:comment": "Describes the products or services offered by a BPP",
  "rdfs:subClassOf": "schema:Collection",
  "rdfs:isDefinedBy": "https://becknprotocol.io/schema/",
  "rdfs:seeAlso": "https://schema.org/Collection",
  "owl:equivalentClass": {
    "@type": "owl:Class",
    "owl:intersectionOf": [
      "schema:Thing",
      {
        "@id": "beckn:id",
        "owl:equivalentProperty": { 
          "@id": "schema:identifier" 
        }
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:descriptor",
        "owl:allValuesFrom": "beckn:Descriptor"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:category",
        "owl:allValuesFrom": "schema:CategoryCode"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:providerId",
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:itemIds",
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:timePeriod",
        "owl:allValuesFrom": "schema:Time"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:labels",
        "owl:allValuesFrom": "schema:PropertyValue"
      }
    ]
  }
}
