{
  "@context": {
    "@vocab": "https://becknprotocol.io/schema/",
    "beckn": "https://becknprotocol.io/schema/",
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#"
  },
  "@id": "https://becknprotocol.io/schema/Catalog",
  "@type": "owl:Class",
  "rdfs:label": "Catalog",
  "rdfs:comment": "Describes the products or services offered by a BPP",
  "rdfs:subClassOf": "schema:Collection",
  "rdfs:isDefinedBy": "https://becknprotocol.io/schema/",
  "rdfs:seeAlso": "https://schema.org/Collection",
  "owl:equivalentClass": {
    "@type": "owl:Class",
    "owl:intersectionOf": [
      "schema:Thing",
      {
        "@id": "beckn:id",
        "rdfs:label": "Catalog ID",
        "rdfs:comment": "Unique identifier for the catalog",
        "owl:equivalentProperty": { 
          "@id": "schema:identifier" 
        }
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:descriptor",
          "rdfs:label": "Catalog Descriptor",
          "rdfs:comment": "Detailed description and information about the catalog"
        },
        "owl:allValuesFrom": "beckn:Descriptor"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:category",
          "rdfs:label": "Catalog Category",
          "rdfs:comment": "The category or classification of the catalog"
        },
        "owl:allValuesFrom": "schema:CategoryCode"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:providerId",
          "rdfs:label": "Provider ID",
          "rdfs:comment": "Reference to the provider who owns this catalog"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:items",
          "rdfs:label": "Items",
          "rdfs:comment": "List of items included in this catalog"
        },
        "owl:allValuesFrom": "beckn:Item"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:timePeriod",
          "rdfs:label": "Catalog Time Period",
          "rdfs:comment": "The time period during which the catalog is valid"
        },
        "owl:allValuesFrom": "schema:Time"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:labels",
          "rdfs:label": "Catalog Labels",
          "rdfs:comment": "Additional labels and metadata for the catalog"
        },
        "owl:allValuesFrom": "schema:PropertyValue"
      }
    ]
  }
}
