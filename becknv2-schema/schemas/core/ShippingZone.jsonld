{
  "@context": {
    "@vocab": "https://becknprotocol.io/schema/",
    "beckn": "https://becknprotocol.io/schema/",
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#"
  },
  "@id": "https://becknprotocol.io/schema/ShippingZone",
  "@type": "owl:Class",
  "rdfs:label": "Shipping Zone",
  "rdfs:comment": "Defines a geographic shipping zone with delivery rates and timeframes",
  "rdfs:subClassOf": "owl:Thing",
  "rdfs:isDefinedBy": "https://becknprotocol.io/schema/",
  "owl:equivalentClass": {
    "@type": "owl:Class",
    "owl:intersectionOf": [
      "owl:Thing",
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:zoneId",
          "rdfs:label": "Zone ID",
          "rdfs:comment": "Unique identifier for the shipping zone"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:zoneName",
          "rdfs:label": "Zone Name",
          "rdfs:comment": "The name or title of the shipping zone (e.g., Local, Regional, International)"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:countries",
          "rdfs:label": "Countries",
          "rdfs:comment": "List of countries included in this shipping zone"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:regions",
          "rdfs:label": "Regions",
          "rdfs:comment": "List of regions or states included in this shipping zone"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:cities",
          "rdfs:label": "Cities",
          "rdfs:comment": "List of cities included in this shipping zone"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:postalCodes",
          "rdfs:label": "Postal Codes",
          "rdfs:comment": "List of postal codes included in this shipping zone"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:baseRate",
          "rdfs:label": "Base Rate",
          "rdfs:comment": "The base shipping rate for this zone"
        },
        "owl:allValuesFrom": "beckn:Price"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:deliveryTime",
          "rdfs:label": "Delivery Time",
          "rdfs:comment": "The estimated delivery time in days for this zone"
        },
        "owl:allValuesFrom": "schema:Integer"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:freeShippingThreshold",
          "rdfs:label": "Free Shipping Threshold",
          "rdfs:comment": "Order value threshold above which shipping becomes free"
        },
        "owl:allValuesFrom": "beckn:Price"
      }
    ]
  }
}
