{
  "@context": {
    "@vocab": "https://becknprotocol.io/schema/",
    "beckn": "https://becknprotocol.io/schema/",
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#"
  },
  "@id": "https://becknprotocol.io/schema/Location",
  "@type": "owl:Class",
  "rdfs:label": "Location",
  "rdfs:comment": "The physical location of something",
  "rdfs:subClassOf": "schema:Place",
  "rdfs:isDefinedBy": "https://becknprotocol.io/schema/",
  "rdfs:seeAlso": "https://schema.org/Place",
  "owl:equivalentClass": {
    "@type": "owl:Class",
    "owl:intersectionOf": [
      "schema:Place",
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "schema:identifier",
          "rdfs:label": "Location Identifier",
          "rdfs:comment": "Unique identifier for the location"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:descriptor",
          "rdfs:label": "Location Descriptor",
          "rdfs:comment": "Detailed description and information about the location"
        },
        "owl:allValuesFrom": "beckn:Descriptor"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:mapUrl",
          "rdfs:label": "Map URL",
          "rdfs:comment": "URL to a map showing the location"
        },
        "owl:allValuesFrom": "schema:URL"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:gps",
          "rdfs:label": "GPS Coordinates",
          "rdfs:comment": "GPS coordinates (latitude and longitude) of the location"
        },
        "owl:allValuesFrom": "beckn:Gps"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "schema:address",
          "rdfs:label": "Location Address",
          "rdfs:comment": "Postal address of the location"
        },
        "owl:allValuesFrom": "schema:PostalAddress"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:city",
          "rdfs:label": "City",
          "rdfs:comment": "City where the location is situated"
        },
        "owl:allValuesFrom": "beckn:City"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:district",
          "rdfs:label": "District",
          "rdfs:comment": "District or administrative division of the location"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:state",
          "rdfs:label": "State",
          "rdfs:comment": "State or province where the location is situated"
        },
        "owl:allValuesFrom": "beckn:State"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:country",
          "rdfs:label": "Country",
          "rdfs:comment": "Country where the location is situated"
        },
        "owl:allValuesFrom": "beckn:Country"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:areaCode",
          "rdfs:label": "Area Code",
          "rdfs:comment": "Area code or postal code for the location"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:circle",
          "rdfs:label": "Circle",
          "rdfs:comment": "Circular boundary definition for the location"
        },
        "owl:allValuesFrom": "beckn:Circle"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:polygon",
          "rdfs:label": "Polygon",
          "rdfs:comment": "Polygonal boundary definition for the location"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:3dspace",
          "rdfs:label": "3D Space",
          "rdfs:comment": "Three-dimensional space definition for the location"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:rating",
          "rdfs:label": "Location Rating",
          "rdfs:comment": "Rating and reviews for the location"
        },
        "owl:allValuesFrom": "beckn:Rating"
      }
    ]
  }
}
