{
  "@context": {
    "@vocab": "https://becknprotocol.io/schema/",
    "beckn": "https://becknprotocol.io/schema/",
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#"
  },
  "@id": "https://becknprotocol.io/schema/Category",
  "@type": "owl:Class",
  "rdfs:label": "Category",
  "rdfs:comment": "A label under which a collection of items can be grouped",
  "rdfs:subClassOf": "schema:CategoryCode",
  "rdfs:isDefinedBy": "https://becknprotocol.io/schema/",
  "rdfs:seeAlso": "https://schema.org/CategoryCode",
  "owl:equivalentClass": {
    "@type": "owl:Class",
    "owl:intersectionOf": [
      "schema:CategoryCode",
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "schema:identifier",
          "rdfs:label": "Category Identifier",
          "rdfs:comment": "Unique identifier for the category"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:parentCategoryId",
          "rdfs:label": "Parent Category ID",
          "rdfs:comment": "Reference to the parent category if this is a subcategory"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:descriptor",
          "rdfs:label": "Category Descriptor",
          "rdfs:comment": "Detailed description and information about the category"
        },
        "owl:allValuesFrom": "beckn:Descriptor"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:time",
          "rdfs:label": "Category Time",
          "rdfs:comment": "Time-related information for the category"
        },
        "owl:allValuesFrom": "schema:Time"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:ttl",
          "rdfs:label": "Category TTL",
          "rdfs:comment": "Time to live for the category information"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:tags",
          "rdfs:label": "Category Tags",
          "rdfs:comment": "Additional tags and metadata for the category"
        },
        "owl:allValuesFrom": "beckn:TagGroup"
      }
    ]
  }
}
