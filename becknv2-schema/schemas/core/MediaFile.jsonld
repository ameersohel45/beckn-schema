{
  "@context": {
    "@vocab": "https://becknprotocol.io/schema/",
    "beckn": "https://becknprotocol.io/schema/",
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#"
  },
  "@id": "https://becknprotocol.io/schema/MediaFile",
  "@type": "owl:Class",
  "rdfs:label": "Media File",
  "rdfs:comment": "This object contains a url to a media file",
  "rdfs:subClassOf": "schema:MediaObject",
  "rdfs:isDefinedBy": "https://becknprotocol.io/schema/",
  "rdfs:seeAlso": "https://schema.org/MediaObject",
  "owl:equivalentClass": {
    "@type": "owl:Class",
    "owl:intersectionOf": [
      "schema:MediaObject",
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:mimetype",
          "rdfs:label": "Media MIME Type",
          "rdfs:comment": "The MIME type of the media file (e.g., image/jpeg, video/mp4)"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:signature",
          "rdfs:label": "Media Signature",
          "rdfs:comment": "Digital signature for verifying the authenticity of the media file"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:dsa",
          "rdfs:label": "Digital Signature Algorithm",
          "rdfs:comment": "The algorithm used for the digital signature"
        },
        "owl:allValuesFrom": "schema:Text"
      }
    ]
  }
}
