{
  "@context": {
    "@vocab": "https://becknprotocol.io/schema/",
    "beckn": "https://becknprotocol.io/schema/",
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#"
  },
  "@id": "https://becknprotocol.io/schema/Fulfillment",
  "@type": "owl:Class",
  "rdfs:label": "Fulfillment",
  "rdfs:comment": "Describes how an order will be rendered/fulfilled to the end-customer",
  "rdfs:subClassOf": "schema:Action",
  "rdfs:isDefinedBy": "https://becknprotocol.io/schema/",
  "rdfs:seeAlso": "https://schema.org/Action",
  "owl:equivalentClass": {
    "@type": "owl:Class",
    "owl:intersectionOf": [
      "schema:Action",
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "schema:identifier",
          "rdfs:label": "Fulfillment Identifier",
          "rdfs:comment": "Unique identifier for the fulfillment process"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:type",
          "rdfs:label": "Fulfillment Type",
          "rdfs:comment": "The type of fulfillment (e.g., home delivery, pickup, dine-in)"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:rateable",
          "rdfs:label": "Fulfillment Rateable",
          "rdfs:comment": "Whether the fulfillment can be rated by customers"
        },
        "owl:allValuesFrom": "schema:Boolean"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:rating",
          "rdfs:label": "Fulfillment Rating",
          "rdfs:comment": "Customer ratings and reviews for the fulfillment service"
        },
        "owl:allValuesFrom": "beckn:Rating"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:state",
          "rdfs:label": "Fulfillment State",
          "rdfs:comment": "Current state of the fulfillment process"
        },
        "owl:allValuesFrom": "beckn:FulfillmentState"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:tracking",
          "rdfs:label": "Fulfillment Tracking",
          "rdfs:comment": "Whether tracking is available for this fulfillment"
        },
        "owl:allValuesFrom": "schema:Boolean"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:customer",
          "rdfs:label": "Fulfillment Customer",
          "rdfs:comment": "Customer information for the fulfillment"
        },
        "owl:allValuesFrom": "beckn:Customer"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:agent",
          "rdfs:label": "Fulfillment Agent",
          "rdfs:comment": "Agent responsible for the fulfillment"
        },
        "owl:allValuesFrom": "beckn:Agent"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:contact",
          "rdfs:label": "Fulfillment Contact",
          "rdfs:comment": "Contact information for the fulfillment"
        },
        "owl:allValuesFrom": "schema:ContactPoint"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:vehicle",
          "rdfs:label": "Fulfillment Vehicle",
          "rdfs:comment": "Vehicle used for the fulfillment"
        },
        "owl:allValuesFrom": "beckn:Vehicle"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:stops",
          "rdfs:label": "Fulfillment Stops",
          "rdfs:comment": "Stops or waypoints in the fulfillment route"
        },
        "owl:allValuesFrom": "beckn:Stop"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:path",
          "rdfs:label": "Fulfillment Path",
          "rdfs:comment": "The path or route for the fulfillment"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:tags",
          "rdfs:label": "Fulfillment Tags",
          "rdfs:comment": "Additional tags and metadata for the fulfillment"
        },
        "owl:allValuesFrom": "beckn:TagGroup"
      }
    ]
  }
}
