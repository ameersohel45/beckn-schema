{
  "@context": {
    "@vocab": "https://becknprotocol.io/schema/",
    "beckn": "https://becknprotocol.io/schema/",
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#"
  },
  "@id": "https://becknprotocol.io/schema/ShippingInfo",
  "@type": "owl:Class",
  "rdfs:label": "Shipping Info",
  "rdfs:comment": "Describes shipping information for retail items including options, costs, and delivery details",
  "rdfs:subClassOf": "schema:Service",
  "rdfs:isDefinedBy": "https://becknprotocol.io/schema/",
  "rdfs:seeAlso": "https://schema.org/Service",
  "owl:equivalentClass": {
    "@type": "owl:Class",
    "owl:intersectionOf": [
      "schema:Service",
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:shippingMethods",
          "rdfs:label": "Shipping Methods",
          "rdfs:comment": "Available shipping methods and their details"
        },
        "owl:allValuesFrom": "beckn:ShippingMethod"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:defaultShippingMethod",
          "rdfs:label": "Default Shipping Method",
          "rdfs:comment": "The default shipping method selected for this item"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:shippingZones",
          "rdfs:label": "Shipping Zones",
          "rdfs:comment": "Geographic shipping zones and their rates"
        },
        "owl:allValuesFrom": "beckn:ShippingZone"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:packagingInfo",
          "rdfs:label": "Packaging Information",
          "rdfs:comment": "Details about packaging, weight, dimensions, and handling requirements"
        },
        "owl:allValuesFrom": "beckn:PackagingInfo"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:returnShipping",
          "rdfs:label": "Return Shipping",
          "rdfs:comment": "Return shipping policies and procedures"
        },
        "owl:allValuesFrom": "beckn:ReturnShipping"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:shippingNotes",
          "rdfs:label": "Shipping Notes",
          "rdfs:comment": "Additional notes and instructions for shipping"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:lastUpdated",
          "rdfs:label": "Last Updated",
          "rdfs:comment": "Timestamp when shipping information was last updated"
        },
        "owl:allValuesFrom": "schema:DateTime"
      }
    ]
  }
}
