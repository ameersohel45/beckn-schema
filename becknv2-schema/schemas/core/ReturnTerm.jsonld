{
  "@context": {
    "@vocab": "https://becknprotocol.io/schema/",
    "beckn": "https://becknprotocol.io/schema/",
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#"
  },
  "@id": "https://becknprotocol.io/schema/ReturnTerm",
  "@type": "owl:Class",
  "rdfs:label": "Return Term",
  "rdfs:comment": "Defines return terms and conditions for retail items",
  "rdfs:subClassOf": "owl:Thing",
  "rdfs:isDefinedBy": "https://becknprotocol.io/schema/",
  "owl:equivalentClass": {
    "@type": "owl:Class",
    "owl:intersectionOf": [
      "owl:Thing",
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:returnType",
          "rdfs:label": "Return Type",
          "rdfs:comment": "The type of return policy (e.g., full refund, exchange, store credit)"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:returnTime",
          "rdfs:label": "Return Time",
          "rdfs:comment": "The time limit for returns (e.g., 7 days, 30 days, 90 days)"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:returnCondition",
          "rdfs:label": "Return Condition",
          "rdfs:comment": "The condition requirements for items to be eligible for return"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:returnEligibility",
          "rdfs:comment": "The criteria that determine if an item can be returned"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:returnProcess",
          "rdfs:label": "Return Process",
          "rdfs:comment": "The steps and procedures for processing returns"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:returnDocumentation",
          "rdfs:label": "Return Documentation",
          "rdfs:comment": "Required documentation for return processing"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:returnRestrictions",
          "rdfs:label": "Return Restrictions",
          "rdfs:comment": "Any limitations or restrictions on returns"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:returnAvailable",
          "rdfs:label": "Return Available",
          "rdfs:comment": "Whether returns are currently available for this item"
        },
        "owl:allValuesFrom": "schema:Boolean"
      }
    ]
  }
}
