{
  "@context": {
    "@vocab": "https://becknprotocol.io/schema/",
    "beckn": "https://becknprotocol.io/schema/",
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#"
  },
  "@id": "https://becknprotocol.io/schema/Price",
  "@type": "owl:Class",
  "rdfs:label": "Price",
  "rdfs:comment": "Describes the price of a product or service",
  "rdfs:subClassOf": "schema:Offer",
  "rdfs:isDefinedBy": "https://becknprotocol.io/schema/",
  "rdfs:seeAlso": "https://schema.org/Offer",
  "owl:equivalentClass": {
    "@type": "owl:Class",
    "owl:intersectionOf": [
      "schema:Offer",
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "schema:priceCurrency",
          "rdfs:label": "Price Currency",
          "rdfs:comment": "The currency in which the price is expressed (e.g., USD, EUR, INR)"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "schema:price",
          "rdfs:label": "Price",
          "rdfs:comment": "The base price of the product or service"
        },
        "owl:allValuesFrom": "schema:Number"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:estimatedValue",
          "rdfs:label": "Estimated Value",
          "rdfs:comment": "The estimated or approximate price value"
        },
        "owl:allValuesFrom": "schema:Number"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:computedValue",
          "rdfs:label": "Computed Value",
          "rdfs:comment": "The calculated price value based on various factors"
        },
        "owl:allValuesFrom": "schema:Number"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:listedValue",
          "rdfs:label": "Listed Value",
          "rdfs:comment": "The originally listed or catalog price"
        },
        "owl:allValuesFrom": "schema:Number"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:offeredValue",
          "rdfs:label": "Offered Value",
          "rdfs:comment": "The current offered or selling price"
        },
        "owl:allValuesFrom": "schema:Number"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:minimumValue",
          "rdfs:label": "Minimum Value",
          "rdfs:comment": "The minimum acceptable price for the product or service"
        },
        "owl:allValuesFrom": "schema:Number"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:maximumValue",
          "rdfs:label": "Maximum Value",
          "rdfs:comment": "The maximum acceptable price for the product or service"
        },
        "owl:allValuesFrom": "schema:Number"
      }
    ]
  }
}
