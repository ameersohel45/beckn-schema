{
  "@context": {
    "@vocab": "https://becknprotocol.io/schema/",
    "beckn": "https://becknprotocol.io/schema/",
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#"
  },
  "@id": "https://becknprotocol.io/schema/Provider",
  "@type": "owl:Class",
  "rdfs:label": "Provider",
  "rdfs:comment": "Describes the business provider",
  "rdfs:subClassOf": "schema:Organization",
  "rdfs:isDefinedBy": "https://becknprotocol.io/schema/",
  "rdfs:seeAlso": "https://schema.org/Organization",
  "owl:equivalentClass": {
    "@type": "owl:Class",
    "owl:intersectionOf": [
      "schema:Organization",
      {
        "@id": "beckn:id",
        "owl:equivalentProperty": { 
          "@id": "schema:identifier" 
        }
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:descriptor",
          "rdfs:label": "Provider Descriptor",
          "rdfs:comment": "Detailed description and information about the provider"
        },
        "owl:allValuesFrom": "beckn:Descriptor"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:timePeriod",
          "rdfs:label": "Provider Time Period",
          "rdfs:comment": "The time period during which the provider is operational"
        },
        "owl:allValuesFrom": "schema:Time"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:locations",
          "rdfs:label": "Provider Locations",
          "rdfs:comment": "Physical locations where the provider operates"
        },
        "owl:allValuesFrom": "beckn:Location"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:rateable",
          "rdfs:label": "Provider Rateable",
          "rdfs:comment": "Whether the provider can be rated by customers"
        },
        "owl:allValuesFrom": "schema:Boolean"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:rating",
          "rdfs:label": "Provider Rating",
          "rdfs:comment": "Customer ratings and reviews for the provider"
        },
        "owl:allValuesFrom": "beckn:Rating"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:labels",
          "rdfs:label": "Provider Labels",
          "rdfs:comment": "Additional labels and metadata for the provider"
        },
        "owl:allValuesFrom": "schema:PropertyValue"
      }
    ]
  }
}
