{
  "@context": {
    "@vocab": "https://becknprotocol.io/schema/",
    "beckn": "https://becknprotocol.io/schema/",
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#"
  },
  "@id": "https://becknprotocol.io/schema/ReturnShipping",
  "@type": "owl:Class",
  "rdfs:label": "Return Shipping",
  "rdfs:comment": "Defines return shipping policies, procedures, and costs",
  "rdfs:subClassOf": "owl:Thing",
  "rdfs:isDefinedBy": "https://becknprotocol.io/schema/",
  "owl:equivalentClass": {
    "@type": "owl:Class",
    "owl:intersectionOf": [
      "owl:Thing",
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:returnPolicy",
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:returnWindow",
        "owl:allValuesFrom": "schema:Integer"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:returnShippingCost",
        "owl:allValuesFrom": "beckn:Price"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:freeReturns",
        "owl:allValuesFrom": "schema:Boolean"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:returnLabelProvided",
        "owl:allValuesFrom": "schema:Boolean"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:returnAddress",
        "owl:allValuesFrom": "schema:PostalAddress"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:returnInstructions",
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:restockingFee",
        "owl:allValuesFrom": "beckn:Price"
      }
    ]
  }
}
