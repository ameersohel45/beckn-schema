{
  "@context": {
    "@vocab": "https://becknprotocol.io/schema/",
    "beckn": "https://becknprotocol.io/schema/",
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#"
  },
  "@id": "https://becknprotocol.io/schema/Dimensions",
  "@type": "owl:Class",
  "rdfs:label": "Dimensions",
  "rdfs:comment": "Defines physical dimensions including length, width, height, and units",
  "rdfs:subClassOf": "owl:Thing",
  "rdfs:isDefinedBy": "https://becknprotocol.io/schema/",
  "owl:equivalentClass": {
    "@type": "owl:Class",
    "owl:intersectionOf": [
      "owl:Thing",
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:length",
          "rdfs:label": "Length",
          "rdfs:comment": "The length dimension of the item"
        },
        "owl:allValuesFrom": "schema:Number"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:width",
          "rdfs:label": "Width",
          "rdfs:comment": "The width dimension of the item"
        },
        "owl:allValuesFrom": "schema:Number"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:height",
          "rdfs:label": "Height",
          "rdfs:comment": "The height dimension of the item"
        },
        "owl:allValuesFrom": "schema:Number"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:unit",
          "rdfs:label": "Unit",
          "rdfs:comment": "The unit of measurement for dimensions (e.g., cm, inches, mm)"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:weight",
          "rdfs:label": "Weight",
          "rdfs:comment": "The weight of the item"
        },
        "owl:allValuesFrom": "schema:Number"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:weightUnit",
          "rdfs:label": "Weight Unit",
          "rdfs:comment": "The unit of measurement for weight (e.g., kg, lbs, grams)"
        },
        "owl:allValuesFrom": "schema:Text"
      }
    ]
  }
}
