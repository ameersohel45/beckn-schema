{
  "@context": {
    "@vocab": "https://becknprotocol.io/schema/",
    "beckn": "https://becknprotocol.io/schema/",
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#"
  },
  "@id": "https://becknprotocol.io/schema/ShippingMethod",
  "@type": "owl:Class",
  "rdfs:label": "Shipping Method",
  "rdfs:comment": "Defines a shipping method with delivery details, carrier information, and costs",
  "rdfs:subClassOf": "owl:Thing",
  "rdfs:isDefinedBy": "https://becknprotocol.io/schema/",
  "owl:equivalentClass": {
    "@type": "owl:Class",
    "owl:intersectionOf": [
      "owl:Thing",
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:methodId",
          "rdfs:label": "Shipping Method ID",
          "rdfs:comment": "Unique identifier for the shipping method"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:methodName",
          "rdfs:label": "Shipping Method Name",
          "rdfs:comment": "The name or title of the shipping method (e.g., Standard, Express, Overnight)"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:carrier",
          "rdfs:label": "Shipping Carrier",
          "rdfs:comment": "The shipping carrier or company providing the service"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:estimatedDeliveryDays",
          "rdfs:label": "Estimated Delivery Days",
          "rdfs:comment": "The estimated number of days for delivery"
        },
        "owl:allValuesFrom": "schema:Integer"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:cost",
          "rdfs:label": "Shipping Cost",
          "rdfs:comment": "The cost of shipping using this method"
        },
        "owl:allValuesFrom": "beckn:Price"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:trackingAvailable",
          "rdfs:label": "Tracking Available",
          "rdfs:comment": "Whether package tracking is available for this shipping method"
        },
        "owl:allValuesFrom": "schema:Boolean"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:insuranceAvailable",
          "rdfs:label": "Insurance Available",
          "rdfs:comment": "Whether shipping insurance is available for this method"
        },
        "owl:allValuesFrom": "schema:Boolean"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "beckn:insuranceCost",
          "rdfs:label": "Insurance Cost",
          "rdfs:comment": "The cost of shipping insurance if available"
        },
        "owl:allValuesFrom": "beckn:Price"
      }
    ]
  }
}
