{
  "@context": {
    "@vocab": "https://becknprotocol.io/schema/",
    "beckn": "https://becknprotocol.io/schema/",
    "grocery": "https://example.org/terms/v1/grocery",
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#"
  },
  "@id": "https://becknprotocol.io/schema/GroceryItem",
  "@type": "owl:Class",
  "rdfs:label": "Grocery Item",
  "rdfs:comment": "Describes a grocery item with food-specific properties",
  "rdfs:subClassOf": "beckn:Item",
  "rdfs:isDefinedBy": "https://becknprotocol.io/schema/",
  "owl:equivalentClass": {
    "@type": "owl:Class",
    "owl:intersectionOf": [
      "beckn:Item",
      {
        "@id": "grocery:groceryItemId",
        "rdfs:label": "Grocery Item ID",
        "rdfs:comment": "Unique identifier for the grocery item",
        "owl:equivalentProperty": { 
          "@id": "beckn:id" 
        }
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "grocery:price",
          "rdfs:label": "Grocery Item Price",
          "rdfs:comment": "Pricing information for the grocery item"
        },
        "owl:allValuesFrom": "beckn:Price"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "grocery:quantity",
          "rdfs:label": "Grocery Item Quantity",
          "rdfs:comment": "Quantity information including count, measure, and units"
        },
        "owl:allValuesFrom": "beckn:ItemQuantity"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "grocery:brand",
          "rdfs:label": "Grocery Item Brand",
          "rdfs:comment": "Brand information for the grocery item"
        },
        "owl:allValuesFrom": "schema:Brand"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "grocery:sku",
          "rdfs:label": "Stock Keeping Unit",
          "rdfs:comment": "Unique identifier for inventory management"
        },
        "owl:allValuesFrom": "schema:sku"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "grocery:expiryDate",
          "rdfs:label": "Expiry Date",
          "rdfs:comment": "Expiration date for perishable grocery items"
        },
        "owl:allValuesFrom": "schema:Date"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "grocery:storageInstructions",
          "rdfs:label": "Storage Instructions",
          "rdfs:comment": "Storage and handling instructions for the grocery item"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "grocery:nutritionalInfo",
          "rdfs:label": "Nutritional Information",
          "rdfs:comment": "Nutritional facts and dietary information"
        },
        "owl:allValuesFrom": "schema:NutritionInformation"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "grocery:organicCertification",
          "rdfs:label": "Organic Certification",
          "rdfs:comment": "Organic certification details if applicable"
        },
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "grocery:shippingInfo",
          "rdfs:label": "Shipping Information",
          "rdfs:comment": "Shipping details and options for the grocery item"
        },
        "owl:allValuesFrom": "beckn:ShippingInfo"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "grocery:fulfillments",
          "rdfs:label": "Fulfillment Options",
          "rdfs:comment": "Available fulfillment methods for the grocery item"
        },
        "owl:allValuesFrom": "beckn:Fulfillment"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "grocery:payments",
          "rdfs:label": "Payment Options",
          "rdfs:comment": "Accepted payment methods for the grocery item"
        },
        "owl:allValuesFrom": "beckn:Payment"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "grocery:addOnIds",
          "rdfs:label": "Add-On Items",
          "rdfs:comment": "Additional products or services that can be added to the grocery item"
        },
        "owl:allValuesFrom": "beckn:AddOn"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "grocery:cancellationTerms",
          "rdfs:label": "Cancellation Terms",
          "rdfs:comment": "Terms and conditions for cancelling orders of this grocery item"
        },
        "owl:allValuesFrom": "beckn:CancellationTerm"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "grocery:refundTerms",
          "rdfs:label": "Refund Terms",
          "rdfs:comment": "Terms and conditions for refunds of this grocery item"
        },
        "owl:allValuesFrom": "beckn:RefundTerm"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "grocery:replacementTerms",
          "rdfs:label": "Replacement Terms",
          "rdfs:comment": "Terms and conditions for replacing this grocery item"
        },
        "owl:allValuesFrom": "beckn:ReplacementTerm"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "grocery:returnTerms",
          "rdfs:label": "Return Terms",
          "rdfs:comment": "Terms and conditions for returning this grocery item"
        },
        "owl:allValuesFrom": "beckn:ReturnTerm"
      }
    ]
  }
}
