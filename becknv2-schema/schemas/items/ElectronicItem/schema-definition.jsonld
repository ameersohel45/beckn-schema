{
  "@context": {
    "@vocab": "https://becknprotocol.io/schema/",
    "beckn": "https://becknprotocol.io/schema/",
    "electronic": "https://example.org/terms/v1/electronics",
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#"
  },
  "@id": "https://becknprotocol.io/schema/ElectronicItem",
  "@type": "owl:Class",
  "rdfs:label": "Electronic Item",
  "rdfs:comment": "Describes an electronic item",
  "rdfs:subClassOf": "beckn:Item",
  "rdfs:isDefinedBy": "https://becknprotocol.io/schema/",
  "owl:equivalentClass": {
    "@type": "owl:Class",
    "owl:intersectionOf": [
      "beckn:Item",
      {
        "@id": "electronic:electronicItemId",
        "rdfs:label": "Electronic Item ID",
        "rdfs:comment": "Unique identifier for the electronic item",
        "owl:equivalentProperty": { 
          "@id": "beckn:id" 
        }
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "electronic:price",
          "rdfs:label": "Electronic Item Price",
          "rdfs:comment": "Pricing information for the electronic item"
        },
        "owl:allValuesFrom": "beckn:Price"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "electronic:quantity",
          "rdfs:label": "Electronic Item Quantity",
          "rdfs:comment": "Quantity information including count, measure, and units"
        },
        "owl:allValuesFrom": "beckn:ItemQuantity"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "electronic:brand",
          "rdfs:label": "Electronic Item Brand",
          "rdfs:comment": "Brand information for the electronic item"
        },
        "owl:allValuesFrom": "schema:Brand"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "electronic:sku",
          "rdfs:label": "Stock Keeping Unit",
          "rdfs:comment": "Unique identifier for inventory management"
        },
        "owl:allValuesFrom": "schema:sku"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "electronic:shippingInfo",
          "rdfs:label": "Shipping Information",
          "rdfs:comment": "Shipping details and options for the electronic item"
        },
        "owl:allValuesFrom": "beckn:ShippingInfo"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "electronic:fulfillments",
          "rdfs:label": "Fulfillment Options",
          "rdfs:comment": "Available fulfillment methods for the electronic item"
        },
        "owl:allValuesFrom": "beckn:Fulfillment"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "electronic:payments",
          "rdfs:label": "Payment Options",
          "rdfs:comment": "Accepted payment methods for the electronic item"
        },
        "owl:allValuesFrom": "beckn:Payment"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "electronic:addOnIds",
          "rdfs:label": "Add-On Items",
          "rdfs:comment": "Additional products or services that can be added to the electronic item"
        },
        "owl:allValuesFrom": "beckn:AddOn"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "electronic:cancellationTerms",
          "rdfs:label": "Cancellation Terms",
          "rdfs:comment": "Terms and conditions for cancelling orders of this electronic item"
        },
        "owl:allValuesFrom": "beckn:CancellationTerm"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "electronic:refundTerms",
          "rdfs:label": "Refund Terms",
          "rdfs:comment": "Terms and conditions for refunds of this electronic item"
        },
        "owl:allValuesFrom": "beckn:RefundTerm"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "electronic:replacementTerms",
          "rdfs:label": "Replacement Terms",
          "rdfs:comment": "Terms and conditions for replacing this electronic item"
        },
        "owl:allValuesFrom": "beckn:ReplacementTerm"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": {
          "@id": "electronic:returnTerms",
          "rdfs:label": "Return Terms",
          "rdfs:comment": "Terms and conditions for returning this electronic item"
        },
        "owl:allValuesFrom": "beckn:ReturnTerm"
      }
    ]
  }
}
