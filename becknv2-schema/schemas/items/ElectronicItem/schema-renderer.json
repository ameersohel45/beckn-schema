{
  "itemType": "ElectronicItem",
  "version": "1.0.0",
  "description": "Simple renderer for ElectronicItem",
  "extends": "becknv2-schema/schemas/items/Item",
  
  "template": "<div class=\"electronic-card\">\n  <div class=\"content\">\n    <h3>{{ item.brand.name }}</h3>\n    <p class=\"sku\">{{ item.sku }}</p>\n    <p class=\"price\">{{ item.price.currency }} {{ item.price.amount }}</p>\n    \n    <button class=\"add-cart\">Add to Cart</button>\n  </div>\n</div>"
}
