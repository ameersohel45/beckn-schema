{
  "@context": {
    "@vocab": "https://becknprotocol.io/schema/",
    "beckn": "https://becknprotocol.io/schema/",
    "schema": "https://schema.org/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#"
  },
  "@id": "https://becknprotocol.io/schema/ItemV2",
  "@type": "owl:Class",
  "rdfs:label": "Item V2",
  "rdfs:comment": "Describes a product or a service offered to the end consumer by the provider",
  "rdfs:subClassOf": "schema:Product",
  "rdfs:isDefinedBy": "https://becknprotocol.io/schema/",
  "rdfs:seeAlso": "https://schema.org/Product",
  "owl:equivalentClass": {
    "@type": "owl:Class",
    "owl:intersectionOf": [
      "schema:Product",
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "schema:identifier",
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:descriptor",
        "owl:allValuesFrom": "beckn:Descriptor"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:catalogIds",
        "owl:allValuesFrom": "beckn:Catalog"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:categories",
        "owl:allValuesFrom": "beckn:Category"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:locations",
        "owl:allValuesFrom": "beckn:Location"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:xinput",
        "owl:allValuesFrom": "beckn:XInput"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:timePeriod",
        "owl:allValuesFrom": "schema:Time"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:rateable",
        "owl:allValuesFrom": "schema:Boolean"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:rating",
        "owl:allValuesFrom": "beckn:Rating"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:ttl",
        "owl:allValuesFrom": "schema:Text"
      },
      {
        "@type": "owl:Restriction",
        "owl:onProperty": "beckn:labels",
        "owl:allValuesFrom": "beckn:Label"
      }
    ]
  }
